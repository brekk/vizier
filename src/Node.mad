renderAttributes :: List NodeAttribute -> String
export renderAttributes = pipe(
  map(attributeToString),
  join("\n"),
)


export type Node = Node(String, List NodeAttribute) | GlobalNode(List NodeAttribute)

export renderNode = where {
  Node(name, attrs) =>
    List.length(attrs) > 0 ? `${name} [${renderAttributes(attrs)}]\n` : ``

  GlobalNode(attrs) =>
    `node [${renderAttributes(attrs)}]\n`
}
/*
  join(
    `\n${
      pipe(
        List.repeat("  "),
        join(""),
      )(i)
    }`,
  ),
  */
repeatIndent = pipe(
  List.repeat("  "),
  join(""),
)
renderNodesWithIndent :: Integer -> List Node -> String
export renderNodesWithIndent = (i, x) => pipe(
  map(renderNode),
  join(``),
)(x)
export renderNodes = renderNodesWithIndent(0)
